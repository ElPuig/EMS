<odoo>
    <data>
        <record model="ir.ui.view" id="view_attendance_group_list">
            <field name="name">ims.attendance_group.list</field>
            <field name="model">ims.attendance_group</field>
            <field name="arch" type="xml">
                <tree>                    
                    <field name="start_time"/>
                    <field name="end_time"/>
                    <field name="teacher"/>
                    <!-- <field name="student"/> -->
                    <field name="study"/>
                    <field name="subject"/>
                    <field name="space"/>
                    <field name="weekday"/>
                    <field name="color" widget="color_picker"/>                                        
                </tree>
            </field>
        </record>   
    </data>
</odoo>