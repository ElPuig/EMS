<odoo>
    <data>
        <record model="ir.ui.view" id="view_attendance_report_student_wizard">
            <field name="name">ims.attendance_report_student.wizard</field>
            <field name="model">ims.attendance_report_student_wizard</field>
            <field name="arch" type="xml">
                <form string="Session">                       
                    <group col="2">                          
                        <group colspan="2">      
                            <field name="student_id"/>
                        </group>

                        <group>
                            <field name="from_date" />
                        </group>
                        <group>
                            <field name="to_date" />
                        </group>
                    </group>

                     <footer>
                        <button name="print" string="Print" type="object" class="btn-primary" icon="fa-print" />
                        <button name="cancel" string="Cancel" special="cancel"/>
                    </footer>
                </form>              
            </field>			
        </record>
    </data>
</odoo>