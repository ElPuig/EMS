<odoo>
    <data>
        <record model="ir.ui.view" id="view_attendance_template_list">
            <field name="name">ims.attendance_template.list</field>
            <field name="model">ims.attendance_template</field>
            <field name="arch" type="xml">
                <tree>                    
                    <!-- <field name="start_time"/> -->
                    <!-- <field name="end_time"/> -->
                    <field name="teacher"/>
                    <field name="student"/>
                    <field name="study"/>
                    <field name="mp"/>
                    <field name="uf"/>
                    <field name="classroom"/>
                    <field name="weekday"/>
                    <field name="color" widget="color_picker"/>
                    <field name="attendance_group"/>                    
                </tree>
            </field>
        </record>   
    </data>
</odoo>