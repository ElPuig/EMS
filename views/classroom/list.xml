<odoo>
    <data>
        <record model="ir.ui.view" id="view_ims_classroom_list">
            <field name="name">ims classroom list</field>
            <field name="model">ims.classroom</field>
            <field name="arch" type="xml">
                <tree expand="1" default_order="code asc">                           
                    <field name="code"/>
