<odoo>
    <data>
        <record model="ir.ui.view" id="view_level_list">
            <field name="name">ems.level.list</field>
            <field name="model">ems.level</field>
            <field name="arch" type="xml">
                <list expand="True" default_order="acronym asc">      
                    <field name="acronym" class="w-25"/>
                    <field name="name"/>                    
                </list>
            </field>
        </record>   
    </data>
</odoo>