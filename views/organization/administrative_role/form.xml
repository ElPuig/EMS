<odoo>
    <data>
        <record id="view_ims_administrative_role_form" model="ir.ui.view">
            <field name="name">administrative_role form</field>
            <field name="model">ims.administrative_role</field>
            <field name="arch" type="xml">
                <form string="administrative_role">
                    <sheet>
                        <group string="Main data">
                            <group>
                                <field name="name"/>
                            </group>
                            <!-- <group colspan="3">
                                <field name="administrative"/>    
                            </group>                             -->
                        </group>  

                        <notebook>               
                            <page string="Administrative">
                                <field name="administrative" widget="many2many">
                                    <tree>
                                        <field name="name"/>
                                        <field name="surname"/>
                                        <field name="corporate_email"/>
                                    </tree>
                                </field>
                            </page>     

                            <page string="Notes">
                                <field name="notes" placeholder="Write down here your notes..."/>
                            </page>
                        </notebook>                  
                    </sheet>	
                </form>
            </field>			
        </record>
    </data>
</odoo>