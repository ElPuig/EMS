<odoo>
    <data>
        <record model="ir.ui.view" id="ims_view_administrative_role_list">
            <field name="name">ims.administrative_role.list</field>
            <field name="model">ims.administrative_role</field>
            <field name="arch" type="xml">
                <tree expand="1" default_order="name asc">      
                    <field name="name" string="Role" />
                    <field name="administratives" string="Assigned to" widget="many2many_tags"/>
                </tree>
            </field>
        </record>   
    </data>
</odoo>