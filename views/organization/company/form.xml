<odoo>
    <data>
        <record model="ir.ui.view" id="view_company_form">
            <field name="name">ims.company.form</field>
            <field name="model">ims.company</field>
            <field name="mode">primary</field>      
            <field name="inherit_id" ref="ims_template_form_with_photo"/>      
            <field name="arch" type="xml">
                <xpath expr="//group[@id='data']" position="inside">                           
                    <group>
                        <field name="vat"/>    
                    </group>                                          
                </xpath>

                <xpath expr="//page[@id='notes']" position="before">         
                    <page string="Contacts">
                        <field name="contacts" mode="kanban" inherit_id="view_provider_kanban" />
                    </page>    
                </xpath>
            </field>			
        </record>
    </data>
</odoo>