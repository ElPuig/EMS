<odoo>
    <data>
        <record model="ir.ui.view" id="view_ims_administrative_list">
            <field name="name">ims administrative list</field>
            <field name="model">ims.administrative</field>
            <field name="mode">primary</field>      
            <field name="inherit_id" ref="ims.view_ims_corporate_person_list"/> 
            <field name="arch" type="xml">  
                <xpath expr="//tree" position="inside">
                    <field name="role"/>    
                </xpath>              
            </field>		      
        </record>   
    </data>
</odoo>