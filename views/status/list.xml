<odoo>
    <data>
        <record model="ir.ui.view" id="status_list">
            <field name="name">ims status list</field>
            <field name="model">ims.status</field>
            <field name="arch" type="xml">
                <tree>                    
                    <field name="code"/>
                    <field name="value"/>                    
                    <field name="notes"/>
                </tree>
            </field>
        </record>   
    </data>
</odoo>