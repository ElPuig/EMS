<odoo>
    <data>
        <record model="ir.ui.view" id="student_list">
            <field name="name">ims student list</field>
            <field name="model">ims.student</field>
            <field name="arch" type="xml">
                <tree>                    
                    <field name="name"/>
                    <field name="surname"/>
                    <field name="email"/>
                </tree>
            </field>
        </record>   
    </data>
</odoo>